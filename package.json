    "moduleFileExtensions": [
      "json",
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ],
    "coverageReporters": [
      "json",
      "lcovonly",
      "text",
      "clover"
